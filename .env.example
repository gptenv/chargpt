# .env.example

# Server config
CHARGPT_PORT=8842

# ChatGPT session token (required)
CHARGPT_ACCESS_TOKEN=PASTE_YOUR_ACCESS_TOKEN_HERE

# Endpoint (normally ChatGPT.com backend)
CHARGPT_BACKEND_API_URL=https://chat.openai.com/backend-api/conversation

# Optional: User Agent spoofing
CHARGPT_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64; rv:122.0) Gecko/20100101 Firefox/122.0

# Optional: Future use
CHARGPT_WORKSPACE_ID=
CHARGPT_DEBUG=true

