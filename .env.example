# .env.example

# === Server Port ===
CHARGPT_PORT=8842

# === Access Token (cascading with default seed) ===
OPENAI_API_KEY=${OPENAI_API_KEY:-""}
CHATGPT_API_KEY=${CHATGPT_API_KEY:-$OPENAI_API_KEY}
CHATGPT_ACCESS_TOKEN=${CHATGPT_ACCESS_TOKEN:-$CHATGPT_API_KEY}
CHARGPT_ACCESS_TOKEN=${CHARGPT_ACCESS_TOKEN:-$CHATGPT_ACCESS_TOKEN}

# === API Base URL (cascading with default seed) ===
OPENAI_API_BASE=${OPENAI_API_BASE:-""}
CHATGPT_API_BASE=${CHATGPT_API_BASE:-$OPENAI_API_BASE}
CHARGPT_API_BASE=${CHARGPT_API_BASE:-$CHATGPT_API_BASE}

# === Proxy Configuration (e.g., Cloudflare WARP SOCKS5) ===
CHARGPT_PROXY_URL=socks5h://warp:1080

# === User Agent ===
CHARGPT_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64; rv:122.0) Gecko/20100101 Firefox/122.0

# === Workspace ID (for ChatGPT Team accounts) ===
CHARGPT_WORKSPACE_ID=

# === Debug Logging ===
CHARGPT_DEBUG=true

